<div class="container">
	<div class="row">
		<table id="example" class="table table-striped table-bordered" cellspacing="0" width="100%">
	        <thead>
	            <tr>
	                <th>Name</th>
	                <th>Age</th>
	                <th>Contact</th>
	                <th>Disease</th>
	            </tr>
	        </thead>
	        <tbody>
	        	{{#each  patientlist}}
		            <tr>
		                <td>{{p_name}}</td>
		                <td>{{age}}</td>
		                <td>{{mobile}}</td>
		                <td>{{disease}}</td>
		            </tr>
		        {{/each}}
	        </tbody>
	</table>
	</div>
</div>

